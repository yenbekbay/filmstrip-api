{
  "name": "filmstrip-api",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/yenbekbay/filmstrip-api",
  "repository": "yenbekbay/filmstrip-api",
  "license": "GPL-3.0",
  "author": {
    "name": "Ayan Yenbekbay",
    "email": "ayan.yenb@gmail.com",
    "url": "http://yenbekbay.me"
  },
  "engines": {
    "node": "6.x.x"
  },
  "scripts": {
    "deploy": "sh ./scripts/deploy.sh",
    "run-job": "node -r dotenv/config -r babel-register scripts/run-job.js",
    "start": "npm run start:server",
    "start:worker": "node -r dotenv/config -r babel-register src/worker",
    "start:server": "nodemon src/server/index.js",
    "build": "npm run clean && babel --out-dir ./build --ignore **/__tests__,**/__mocks__ ./src",
    "clean": "rimraf build && mkdir build",
    "test": "npm-run-all --parallel typecheck lint jest",
    "jest": "jest",
    "jest:coverage": "jest --coverage",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "typecheck": "flow check ./src",
    "commitmsg": "validate-commit-msg",
    "prepush": "npm test"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "setupFiles": [
      "scripts/jest-env.js"
    ],
    "testEnvironment": "node",
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "testRegex": "/__tests__/.*-test\\.js$"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "maxSubjectLength": 72,
      "types": "conventional-commit-types"
    }
  },
  "dependencies": {
    "agenda": "^0.9.0",
    "babel-runtime": "^6.18.0",
    "body-parser": "^1.15.2",
    "bytes": "^2.4.0",
    "cors": "^2.8.1",
    "dataloader": "^1.2.0",
    "date-fns": "^1.13.0",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "graphql": "^0.7.0",
    "graphql-anywhere": "^1.0.0",
    "graphql-server-express": "^0.4.3",
    "graphql-tag": "^1.1.2",
    "graphql-tools": "^0.8.3",
    "lodash": "^4.17.2",
    "mongodb": "^2.2.12",
    "morgan": "^1.7.0",
    "movie-api": "^0.14.0",
    "promise-throttle": "^0.3.1",
    "request": "^2.79.0",
    "request-promise-native": "^1.0.3",
    "slugify": "^1.0.2",
    "thepiratebay": "^1.3.2",
    "type-detect": "^4.0.0",
    "winston": "^2.3.0",
    "winston-papertrail": "^1.0.4"
  },
  "devDependencies": {
    "@types/agenda": "^0.8.33",
    "@types/body-parser": "^0.0.33",
    "@types/bytes": "^2.4.33",
    "@types/cors": "^0.0.33",
    "@types/date-fns": "^2.5.3",
    "@types/dotenv": "^2.0.19",
    "@types/express": "^4.0.34",
    "@types/graphql": "^0.8.2",
    "@types/inquirer": "^0.0.32",
    "@types/jest": "^16.0.1",
    "@types/lodash": "^4.14.42",
    "@types/mongodb": "^2.1.36",
    "@types/morgan": "^1.7.32",
    "@types/request": "^0.0.36",
    "@types/request-promise-native": "^1.0.2",
    "@types/rimraf": "^0.0.28",
    "@types/type-detect": "^0.1.28",
    "@types/winston": "^0.0.32",
    "babel-cli": "^6.18.0",
    "babel-jest": "^17.0.0",
    "babel-plugin-lodash": "^3.2.10",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "~3.9.1",
    "eslint-config-anvilabs": "^2.0.1",
    "flow-bin": "^0.36.0",
    "husky": "^0.11.9",
    "inquirer": "^1.2.3",
    "jest": "^17.0.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.2",
    "rimraf": "^2.5.4",
    "validate-commit-msg": "^2.8.2"
  }
}
